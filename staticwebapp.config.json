{
    "routes": [
        {
            "route": "/index.html",
            "headers": {
                "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate"
            }
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css,scss,js,png,gif,ico,jpg,svg}"
        ]
    }
}